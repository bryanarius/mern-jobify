 {name, email, password, isMember} = values\n        if(!email || !password || (!isMember && !name)){\n            displayAlert()\n            return\n        }\n        const currentUser = { name, email, password}\n        if(isMember){\n            setupUser({\n            currentUser, \n            endPoint:'login',\n            alertText: 'Login Successful! Redirecting...',\n        })\n        } \n        else {\n            setupUser({\n                currentUser, \n                endPoint:'register',\n                alertText: 'User Created! Redirecting...',\n            })\n        }\n\n    }\n\n    useEffect(()=> {\n        if(user){\n            setTimeout(()=>{\n                navigate('/')\n            },3000)\n            \n        }\n    },[user,navigate])\n\n  return (\n    <Wrapper className='full-page'>\n        <form className='form' onSubmit={onSubmit}>\n            <Logo />\n            <h3>{values.isMember ? 'Login' : 'Register'}</h3>\n\n            {showAlert && <Alert/>}\n            {/* Name Input */}\n            {!values.isMember && (\n                <FormRow \n                type=\"text\"\n                name=\"name\" \n                value={values.name}\n                handleChange={handleChange}\n            />\n            )}\n            \n            {/* Email Input */}\n            <FormRow \n                type=\"email\"\n                name=\"email\" \n                value={values.email}\n                handleChange={handleChange}\n            />\n            {/* Password Input */}\n            <FormRow \n                type=\"password\"\n                name=\"password\" \n                value={values.password}\n                handleChange={handleChange}\n            />\n            <button type=\"submit\" className=\"btn btn-block\" disabled={isLoading}>\n                submit\n            </button>\n            <p>\n                {values.isMember ? 'Not a member yet?': 'Already a member?'}\n                <button type=\"button\" onClick={toggleMember} className='member-btn'>\n                    {values.isMember ? 'Register' : \"Login\"}\n                </button>\n            </p>\n        </form>\n    </Wrapper>\n  )\n}\n\nexport default Register","/Users/bryanarius/Desktop/projects/mern-jobify/client/src/assets/wrappers/ErrorPage.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/assets/wrappers/RegisterPage.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/components/FormRow.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/components/Alert.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/context/appContext.js",["157","158"],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/context/reducer.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/context/actions.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/pages/dashboard/index.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/pages/dashboard/AllJobs.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/pages/dashboard/Profile.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/pages/dashboard/AddJob.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/pages/dashboard/Stats.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/pages/dashboard/SharedLayout.js",["159"],[],"import { Outlet, Link} from 'react-router-dom'\nimport Wrapper from '../../assets/wrappers/SharedLayout'\nimport {Navbar, BigSidebar, SmallSidebar} from '../../components'\n\nconst SharedLayout = () => {\n  return (\n    <Wrapper>\n      <main className=\"dashboard\">\n        <SmallSidebar />\n        <BigSidebar />\n        <div>\n          <Navbar/>\n          <div className='dashboard-page'>\n            <Outlet />\n          </div>\n        </div>\n      </main>      \n    </Wrapper>\n    \n  )\n}\n\nexport default SharedLayout","/Users/bryana