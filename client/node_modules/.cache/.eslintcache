.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/assets/wrappers/LandingPage.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/components/index.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/components/Logo.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/pages/index.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/pages/Error.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/pages/Dashboard.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/pages/Register.js",["90","91"],[],"import {useState, useEffect} from 'react'\nimport {Logo, FormRow, Alert} from '../components'\nimport Wrapper from '../assets/wrappers/RegisterPage'\nimport { useAppContext } from '../context/appContext'\n\nconst initialState = {\n    name: '',\n    email: '',\n    password: '',\n    isMember: true,\n}\n\nconst Register = () => {\n    const [values,setValues] = useState(initialState)\n    // global state and useNavigate\n    const {isLoading, showAlert, displayAlert} = useAppContext()\n\n    const toggleMember = () => {\n        setValues({ ...values,isMember: !values.isMember })\n    }\n\n    const handleChange = (e) => {\n        setValues({...values,[e.target.name]: e.target.value})\n    }\n\n    const onSubmit = (e) => {\n        e.preventDefault()\n        const {name, email, password, isMember} = values\n        if(!email || !password || (!isMember && !name)){\n            displayAlert()\n            return\n        }\n        console.log(values)\n    }\n  return (\n    <Wrapper className='full-page'>\n        <form className='form' onSubmit={onSubmit}>\n            <Logo />\n            <h3>{values.isMember ? 'Login' : 'Register'}</h3>\n\n            {showAlert && <Alert/>}\n            {/* Name Input */}\n            {!values.isMember && (\n                <FormRow \n                type=\"text\"\n                name=\"name\" \n                value={values.name}\n                handleChange={handleChange}\n            />\n            )}\n            \n            {/* Email Input */}\n            <FormRow \n                type=\"email\"\n                name=\"email\" \n                value={values.email}\n                handleChange={handleChange}\n            />\n            {/* Password Input */}\n            <FormRow \n                type=\"password\"\n                name=\"password\" \n                value={values.password}\n                handleChange={handleChange}\n            />\n            <button type=\"submit\" className=\"btn btn-block\">\n                submit\n            </button>\n            <p>\n                {values.isMember ? 'Not a member yet?': 'Already a member?'}\n                <button type=\"button\" onClick={toggleMember} className='member-btn'>\n                    {values.isMember ? 'Register' : \"Login\"}\n                </button>\n            </p>\n        </form>\n    </Wrapper>\n  )\n}\n\nexport default Register","/Users/bryanarius/Desktop/projects/mern-jobify/client/src/assets/wrappers/ErrorPage.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/assets/wrappers/RegisterPage.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/components/FormRow.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/components/Alert.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/context/appContext.js",["92"],[],"import React, {useState, useReducer,useContext} from 'react'\n\nimport reducer from './reducer'\n\nimport { DISPLAY_ALERT, CLEAR_ALERT } from './actions'\n\nconst initialState = {\n    isLoading: false,\n    showAlert: false,\n    alertText: '',\n    alertType: '',\n}\n\nconst AppContext = React.createContext()\n\nconst AppProvider = ({children}) =>{\n    const [state,dispatch] = useReducer(reducer, initialState)\n\nconst displayAlert = () => {\n    dispatch({ type: DISPLAY_ALERT})\n    clearAlert()\n}\n\nconst clearAlert = () => {\n    setTimeout(()=> {\n        dispatch({ type: CLEAR_ALERT})\n    }, 3000)\n}\n    return (\n        <AppContext.Provider value={{...state, displayAlert}}>{children}</AppContext.Provider>\n        )\n}\n\nconst useAppContext = () =>{\n    return useContext(AppContext)\n}\n\nexport {AppProvider, initialState, useAppContext}","/Users/bryanarius/Desktop/projects/mern-jobify/client/src/context/reducer.js",[],[],"/Users/bryanarius/Desktop/projects/mern-jobify/client/src/context/actions.js",[],[],{"ruleId":"93","severity":1,"message":"94","line":1,"column":19,"nodeType":"95","messageId":"96","endLine":1,"endColumn":28},{"ruleId":"93","severity":1,"message":"97","line":16,"column":12,"nodeType":"95","messageId":"96","endLine":16,"endColumn":21},{"ruleId":"93","severity":1,"message":"98","line":1,"column":16,"nodeType":"95","messageId":"96","endLine":1,"endColumn":24},"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'isLoading' is assigned a value but never used.","'useState' is defined but never used."]